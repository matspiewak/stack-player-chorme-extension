{
    "name": "StackPlayer",
    "description": "Extension for stacking video/music links for easy access from any site",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
      "service_worker": "background.js"
    },
    "permissions": ["storage", "activeTab", "scripting"],
    "action": {
      "default_popup": "popup.html"
    }
  }